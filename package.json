  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "request": "~2.40.0",
    "jquery": "~2.1.1",
    "express": "~4.8.4",
    "coffee-script": "~1.7.1",
    "jade": "~1.5.0"
  },
  "devDependencies": {
    "node-webkit-builder": "~0.1.1"
  }
